{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "babel src -d dist --extensions \".js,.mjs\"",
    "clean:build": "npm run clean && npm run build",
    "start": "nodemon",
    "serve": "node dist/index.js",
    "test": "mocha --require @babel/register",
    "migrate:up:dev": "npx sequelize-cli db:migrate --env development",
    "migrate:up:test": "npx sequelize-cli db:migrate --env test",
    "migrate:up:prod": "npx sequelize-cli db:migrate --env production",
    "migrate:undo:dev": "npx sequelize-cli db:migrate:undo --env development",
    "migrate:undo:test": "npx sequelize-cli db:migrate:undo --env test",
    "migrate:undo:prod": "npx sequelize-cli db:migrate:undo --env production",
    "seed:up:dev": "npx sequelize-cli db:seed:all --env development",
    "seed:up:test": "npx sequelize-cli db:seed:all --env test",
    "seed:up:prod": "npx sequelize-cli db:seed:all --env production",
    "seed:undo:dev": "npx sequelize-cli db:seed:undo --env development",
    "seed:undo:test": "npx sequelize-cli db:seed:undo --env test",
    "seed:undo:prod": "npx sequelize-cli db:seed:undo --env production"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.3",
    "@babel/node": "^7.22.19",
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/register": "^7.22.15",
    "chai": "^4.3.10",
    "dotenv": "^16.3.1",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "sequelize-cli": "^6.6.2"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "express-rate-limit": "^7.1.4",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.0",
    "winston": "^3.11.0"
  }
}
